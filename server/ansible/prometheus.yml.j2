# {{ ansible_managed }}
# Example: https://github.com/prometheus/prometheus/blob/master/documentation/examples/prometheus.yml

scrape_configs:
  - job_name: prometheus
    static_configs:
      - targets: ['localhost:9090']

  - job_name: server
    static_configs:
      - targets: 
        - 'host.docker.internal:{{ node_exporter_port }}'
        - 'cadvisor:8080'
